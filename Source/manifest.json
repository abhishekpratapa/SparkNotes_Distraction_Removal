{
  "manifest_version": 2,
  "name": "Close all Distractions on Spark Notes",
  "version": "0.1",
  "content_scripts": [
	  {
	    "matches": [
	      "https://nfs.sparknotes.com/*",
        "http://nfs.sparknotes.com/*",
        "https://sparknotes.com/*",
        "http://sparknotes.com/*",
	      "https://www.sparknotes.com/*",
        "http://www.sparknotes.com/*",
        "https://community.sparknotes.com/*",
	      "http://community.sparknotes.com/*"
	    ],
	    "js": [ "jquery-2.2.4.js", "content.js"]
	  }
  ],
  "browser_action": {
    "default_icon": "images/icon48.png"
  },
  "icons": { 
  	  "16": "images/icon16.png",  
      "48": "images/icon48.png",
      "128": "images/icon128.png" 
    }
}
